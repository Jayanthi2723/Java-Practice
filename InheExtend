public class InheExtend {
	String name;
	String color;
}
class Apple extends InheExtend{
	double price;
	public Apple(String name, String color, double price) {
		this.name=name;
		this.color=color;
		this.price=price;
	}
	public static void main(String[] args) {
		Apple a1=new Apple("Goa", "Green",40.00);
		System.out.println(a1.name);
		System.out.println(a1.color);
		System.out.println(a1.price);
	}
}
